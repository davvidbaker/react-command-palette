{
  "name": "react-commander",
  "version": "0.1.1",
  "description": "An inflexible command palette probably for my own use.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "start": "npm run storybook",
    "clean": "rm -rf lib && mkdir lib",
    "lib": "npm run clean && babel src -d lib",
    "prepare": "npm run lib",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davvidbaker/react-commander.git"
  },
  "keywords": [
    "react",
    "command",
    "palette",
    "wizard",
    "commander"
  ],
  "author": "David Baker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davvidbaker/react-commander/issues"
  },
  "homepage": "https://github.com/davvidbaker/react-commander#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^4.1.3",
    "babel-plugin-styled-components": "^1.10.0",
    "downshift": "^3.1.8",
    "fuzzaldrin-plus": "^0.6.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-modal": "^3.8.1",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0",
    "react-modal": "^3.8.1",
    "fuzzaldrin-plus": ">=0.6.0",
    "downshift": "^3.1.8",
