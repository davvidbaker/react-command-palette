{
  "name": "react-commander",
  "version": "0.2.0",
  "description": "An inflexible command palette probably for my own use.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "start": "npm run storybook",
    "clean": "rm -rf lib && mkdir lib",
    "lib": "npm run clean && babel src -d lib",
    "prepare": "npm run lib",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davvidbaker/react-commander.git"
  },
  "keywords": [
    "react",
    "command",
    "palette",
    "wizard",
    "commander"
  ],
  "author": "David Baker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davvidbaker/react-commander/issues"
  },
  "homepage": "https://github.com/davvidbaker/react-commander#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^4.1.13",
    "babel-plugin-styled-components": "^1.10.0",
    "downshift": "^3.2.6",
    "eslint": "^5.14.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "fuzzaldrin-plus": "^0.6.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-modal": "^3.8.1",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=16.8.3",
    "react-dom": ">=16.8.3",
    "react-modal": "^3.8.1",
    "fuzzaldrin-plus": ">=0.6.0",
    "downshift": "^3.1.8",
    "styled-components": "^4.1.3"
  },
  "dependencies": {
    "xstate": "^4.3.3"
  }
}
